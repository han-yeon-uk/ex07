<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.mapper.ProductMapper">
 <select id="list" resultType="com.example.domain.ProductVO">
 	select*from tbl_product limit #{pageStart},#{perPageNum}
 </select>
 <insert id="insert">
 	insert into tbl_product(pname,price,image) 
 	values(#{pname},#{price},#{image})
 </insert>
  <select id="totalCount" resultType="int">
 	select count(*)from tbl_product limit #{pageStart},#{perPageNum}
 </select>
</mapper>